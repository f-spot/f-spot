<Project>
  <Import Project="fspot.props" />

  <PropertyGroup>
    <FSpotTargetFramework>net472</FSpotTargetFramework>
    <LangVersion>latest</LangVersion>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <PlatformTarget Condition="$(IsWindows)">x86</PlatformTarget>
    <PlatformTarget Condition="!$(IsWindows)">Any CPU</PlatformTarget>
  </PropertyGroup>



  <!-- Assembly Info -->
  <PropertyGroup>
    <Version>0.9.0</Version>
    <AssemblyVersion>0.9.0.0</AssemblyVersion>
    <Title>F-Spot</Title>
    <Description>Personal photo management</Description>
    <Copyright>Copyright (C) 2003-2019 Novell Inc. and others</Copyright>
  </PropertyGroup>
</Project>
