  <ItemGroup>
    <ProjectReference Include="..\..\..\Core\FSpot.Core\FSpot.Core.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\Clients\FSpot\FSpot.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\Core\FSpot.Utils\FSpot.Utils.csproj">
